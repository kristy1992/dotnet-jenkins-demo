{
	"$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
	"contentVersion": "1.0.0.0",
	"parameters":{
		"subscription": {
			"type": "string"
		},
		"hostingPlanName": {
			"type": "string"
		},
		"sku": {
			"type": "string"
		},
		"skucode": {
			"type": "string"
		},
		"workerSize": {
			"type": "string"
		},
		"workerSizeId": {
			"type": "string"
		},
		"numberOfWorkers": {
			"type": "string"
		},
		"hostingEnvironment": {
			"type": "string"
		},
		"cosmosDbName": {
			"type": "string"
		},
		"defaultExperience": {
			"type": "string"
		},
		"cosmos_offerType": {
			"type": "string"
		},
		"locationName": {
			"type": "string"
		},
		"pairedLocationName": {
			"type": "string"
		},
		"pairedLocation": {
			"type": "string"
		},
		"_artifactsLocation": {
			"type": "string",
			"defaultValue": "empty"
		},
		"_artifactsLocationSasToken": {
			"type": "securestring",
			"defaultValue": "empty"
		},
		"storageAccountName": {
			"type": "string"
		},
		"storageAccountResourceGroup": {
			"type": "string"
		},
		"questDeployPackageFileName": {
			"type": "string",
			"minLength": 1
		},
		"questWebsiteName": {
			"type": "string"
		},
		"sourceSlot": {
			"type": "string"
		},
		"destinationSlot": {
			"type": "string"
		},
		"isNew": {
			"type": "bool",
			"defaultValue": false
		},
		"isDeploy": {
			"type": "bool",
			"defaultValue": true
			},
		"cloudServiceName": {
		  "type": "string"
		},
		"cloudServiceSlotName": {
		  "type": "string"
		},
		"cloudServiceDeploymentLabel": {
		  "type": "string"
		},
		"cloudServicePackageFileName": {
		  "type": "string"
		},
		"cloudServiceConfigurationFileName": {
		  "type": "string"
		},
		"cloudServiceDeploymentOptions": {
		  "type": "string"
		},
		"deployCloudService": {
		  "type": "bool",
		  "defaultValue": false
		}
	},
	"resources": [{
			"apiVersion": "2015-04-08",
			"kind": "GlobalDocumentDB",
			"type": "Microsoft.DocumentDb/databaseAccounts",
			"name": "[parameters('cosmosDbName')]",
			"location": "[resourceGroup().location]",
			"properties": {
				"databaseAccountOfferType": "[parameters('cosmos_offerType')]",
				"enableMultipleWriteLocations": true,
				"isVirtualNetworkFilterEnabled": false,
				"enableAutomaticFailover": true,
				"virtualNetworkRules": [],
				"ipRangeFilter": "",
				"locations": [{
						"id": "[concat(parameters('cosmosDbName'), '-', resourceGroup().location)]",
						"failoverPriority": 0,
						"locationName": "[parameters('locationName')]"
					},
					{
						"id": "[concat(parameters('cosmosDbName'), '-', parameters('pairedLocation'))]",
						"failoverPriority": 1,
						"locationName": "[parameters('pairedLocationName')]"
					}
				],
				"dependsOn": []
			},
			"tags": {
				"defaultExperience": "[parameters('defaultExperience')]"
			}
		},
		{
			"apiVersion": "2016-09-01",
			"name": "[parameters('hostingPlanName')]",
			"type": "Microsoft.Web/serverfarms",
			"location": "[resourceGroup().location]",
			"sku": {
				"Tier": "[parameters('sku')]",
				"Name": "[parameters('skuCode')]"
			}
		},
		{
			"apiVersion": "2018-02-01",
			"name": "[parameters(websiteName')]",
			"type": "Microsoft.Web/sites",
			"location": "[resourceGroup().location]",
			"dependsOn": [
				"[concat('Microsoft.Web/serverfarms/', parameters('hostingPlanName'))]"
			],
			"properties": {
				"serverFarmId": "[resourceId('Microsoft.Web/serverfarms', parameters('hostingPlanName'))]"
			}
		}
	]
	
}